{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.gitmodules": true,
    "**/*.booproj": true,
    "**/*.pidb": true,
    "**/*.suo": true,
    "**/*.user": true,
    "**/*.userprefs": true,
    "**/*.unityproj": true,
    "**/*.dll": true,
    "**/*.exe": true,
    "**/*.pdf": true,
    "**/*.mid": true,
    "**/*.midi": true,
    "**/*.wav": true,
    "**/*.gif": true,
    "**/*.ico": true,
    "**/*.psd": true,
    "**/*.tga": true,
    "**/*.3ds": true,
    "**/*.3DS": true,
    "**/*.fbx": true,
    "**/*.FBX": true,
    "**/*.lxo": true,
    "**/*.LXO": true,
    "**/*.ma": true,
    "**/*.MA": true,
    "**/*.obj": true,
    "**/*.OBJ": true,
    "**/*.asset": true,
    "**/*.cubemap": true,
    "**/*.flare": true,
    "**/*.mat": true,
    "build/": true,
    "Build/": true,
    "Library/": true,
    "library/": true,
    "obj/": true,
    "Obj/": true,
    "ProjectSettings/": true,
    "UserSettings/": true,
    "temp/": true,
    "Temp/": true,
    "Logs/": true,
    ".vscode/": true,
    "*.csproj/": true,
    "**/*.sln/": true,
    "**/*.meta": true,
  },
  "files.watcherExclude": {
    "**/Library/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "files.autoSave": "afterDelay",
  "csharp.referencesCodeLens.enabled": false,
  "gitdoc.enabled": true,
  "gitdoc.autoCommitDelay": 15000,
  "gitdoc.autoPush": "onCommit",
  "gitdoc.autoPushDelay": 10000,
  "gitdoc.commitValidationLevel": "none",
  "git.enabled": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "git.autoStash": true
}